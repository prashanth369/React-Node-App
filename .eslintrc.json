{
    "parser": "babel-eslint",
    "extends": ["airbnb"],
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "no-console": "off",
      "comma-dangle": "off",
      "import/no-unresolved" : "off",
      "operator-linebreak" : "off",
      "semi": ["off", "never"],
      "react/jsx-filename-extension": "off",
      "import/prefer-default-export": "off",
      "jsx-a11y/label-has-associated-control": ["error", {
        "required": {
          "some": ["nesting", "id"]
        }
      }],
      "jsx-a11y/label-has-for": "off",
      "indent": "off",
      "import/extensions": [
        "error",
        {
            "js": "ignorePackages"
        }
    ]
    }
  }