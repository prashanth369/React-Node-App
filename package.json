{
  "name": "react-node-app",
  "version": "1.0.0",
  "description": "A web Application with React and Node",
  "main": "src/server/index.js",
  "type": "module",
  "scripts": {
    "build": "webpack --mode production --config webpack.config.cjs",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot --config webpack.config.cjs",
    "server": "nodemon",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "lint": "eslint ./src  --ext .tsx  --ext .js --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prashanth369/React-Node-App.git"
  },
  "author": "Prashanth Reddy Somireddy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/prashanth369/React-Node-App/issues"
  },
  "homepage": "https://github.com/prashanth369/React-Node-App#readme",
  "dependencies": {
    "axios": "^0.25.0",
    "babel-polyfill": "^6.26.0",
    "express": "^4.16.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "ts-node": "^10.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.16.7",
    "@types/express": "^4.17.13",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "^2.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.3",
    "style-loader": "^0.23.0",
    "ts-loader": "^3.2.0",
    "typescript": "^4.5.5",
    "url-loader": "^1.0.1",
